{
  "name": "node_typeorm",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.ts",
  "scripts": {
    "build": "npx -y tsc",
    "dev": "env $(cat .env) tsnd --transpile-only src/server.ts",
    "migration": "env $(cat .env) typeorm-ts-node-commonjs -d ./src/database/config/data-source.ts",
    "migration:generate": "npm run migration -- migration:generate ./src/database/migrations/default",
    "migration:run": "npm run migration -- migration:run",
    "db:create": "env $(cat .env) ts-node ./node_modules/typeorm-extension/dist/cli/index.js db:create -d ./src/database/config/data-source.ts",
    "db:drop": "env $(cat .env) ts-node ./node_modules/typeorm-extension/dist/cli/index.js db:drop -d ./src/database/config/data-source.ts",
    "seed": "env $(cat .env) ts-node ./node_modules/typeorm-extension/dist/cli/index.js seed -d ./src/database/config/data-source.ts",
    "start": "env $(cat .env) node ./dist/server.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GabrielCoruja/node_typeorm.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GabrielCoruja/node_typeorm/issues"
  },
  "homepage": "https://github.com/GabrielCoruja/node_typeorm#readme",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.15.10",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.12",
    "typeorm-extension": "^2.6.2"
  }
}
